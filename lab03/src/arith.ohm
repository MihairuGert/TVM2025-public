Arithmetic {
    Sum = NonemptyListOf<Sub, "+"> 
    Sub = NonemptyListOf<Mul, "-">
    Mul = NonemptyListOf<Div, "*">
    Div = NonemptyListOf<UnaryMin, "/">
    
    UnaryMin = ("-")* Atom 
    
    Atom = 
      | "(" Sum ")" -- parenthesis 
      | number
    
    number = digit+ --number 
            | letter+ --variable
}
